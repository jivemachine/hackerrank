# loops
if __name__ == '__main__':
    n = int(input())
    for i in range(n):
        print(i ** 2)

# Write a function
def is_leap(year):
    leap = False
    
    # Write your logic here
    if year % 400 == 0:
        leap = True
    elif year % 100 == 0:
        leap = False
    elif year % 4 == 0:
        leap = True 
    else:
        leap = False           
    return leap

year = int(input())

# Print function
if __name__ == '__main__':
    nums = []
    while True:
        n = int(input())
        if n <= 1:
            continue
        elif n >= 150:
            continue
        else:
            # number was succesfully parsed
            # break loop
            break
    for x in range(1 , n + 1):
        print(x, end="") 

        