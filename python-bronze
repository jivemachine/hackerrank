# loops
if __name__ == '__main__':
    n = int(input())
    for i in range(n):
        print(i ** 2)

# Write a function
def is_leap(year):
    leap = False
    
    # Write your logic here
    if year % 400 == 0:
        leap = True
    elif year % 100 == 0:
        leap = False
    elif year % 4 == 0:
        leap = True 
    else:
        leap = False           
    return leap

year = int(input())

